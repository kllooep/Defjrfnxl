local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()

local Window = redzlib:MakeWindow({
Title = "DemoZHub",
SubTitle = "بواسطة رامي",
SaveFolder = "testando | redz lib v5.lua"
})

Window:AddMinimizeButton({
Button = { Image = "rbxassetid://71014873973869", BackgroundTransparency = 0 },
Corner = { CornerRadius = UDim.new(35, 1) },
})
local MusicTab = Window:MakeTab({
    Name = "(gamepass)أغاني",
    Icon = "music"
})

-- دالة عامة لتشغيل الموسيقى
local function PlayMusic(id)
    local args1 = { [1] = "SkateBoard" }
    local args2 = { [1] = "PickingScooterMusicText", [2] = id }
    local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("RE"):FindFirstChild("1NoMoto1rVehicle1s")
    if Remote then
        pcall(function()
            Remote:FireServer(unpack(args1))
            Remote:FireServer(unpack(args2))
        end)
    end
end

-- ==================== قسم: إدخال الكود يدويًا ====================
