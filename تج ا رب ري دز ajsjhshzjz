local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()

local Window = redzlib:MakeWindow({
    Name = "NetroZ Music",
    SubTitle = "by Rami",
    SaveFolder = "NetroZHubMusic"
})

Window:AddMinimizeButton({
Button = { Image = "rbxassetid://126128591607444", BackgroundTransparency = 0 },
Corner = { CornerRadius = UDim.new(35, 1) },
})

local TabMusic = Window:MakeTab({
    Title = "اغاني مجانية",
    Icon = "radio"
})

TabMusic:AddSection("اختيار الصوت")


