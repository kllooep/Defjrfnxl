local audioLoop = false

local function playLoopedAudio()
    if not selectedAudioID then
        TabMusic:AddNotification({
            Name = "ØªØ­Ø°ÙŠØ±",
            Content = "Ù„Ù… ÙŠØªÙ… Ø§Ø®ØªÙŠØ§Ø± ØµÙˆØª!",
            Time = 5
        })
        return
    end

    getgenv().ChaosHub_AudioLoop = true

    while getgenv().ChaosHub_AudioLoop do
        local args = {
            [1] = workspace,
            [2] = selectedAudioID,
            [3] = 1
        }
        local GunSound = game:GetService("ReplicatedStorage"):FindFirstChild("1Gu1nSound1s")
        if GunSound then
            pcall(function()
                GunSound:FireServer(unpack(args))
            end)
        end

        task.spawn(function()
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://" .. selectedAudioID
            sound.Parent = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") or workspace
            sound:Play()
            task.delay(0.5, function()
                if sound and sound.Parent then
                    sound:Destroy()
                end
            end)
        end)

        task.wait(0.5)
    end
end

