-- استخراج الأسماء للقائمة المنسدلة
local audioNames = {}
for _, audio in ipairs(audios) do
    table.insert(audioNames, audio.Name)
end


TabMusic:AddTextBox({
    Name = "ادخال صوت الموسيقى",
    PlaceholderText = "اكتب ID الصوت",
    Callback = function(value)
        local id = tonumber(value)
        if id then
            selectedAudioID = id
            
            TabMusic:AddNotification({
                Name = "تم التحديث",
                Content = "تم تعيين الصوت إلى ID: " .. id,
                Time = 3
            })
        else
            TabMusic:AddNotification({
                Name = "خطأ",
                Content = "الرجاء إدخال رقم صحيح!",
                Time = 5
            })
        end
    end
})


TabMusic:AddDropdown({
    Name = "اختار صوت",
    Options = audioNames,
    Default = audioNames[1],
    Callback = function(value)
        for _, audio in ipairs(audios) do
            if audio.Name == value then
                selectedAudioID = audio.ID
                break
            end
        end
    end
})


TabMusic:AddSection("تشغيل الصوت (مرة واحدة)")

local function playAudio()
    if not selectedAudioID then
        TabMusic:AddNotification({
            Name = "خطأ",
            Content = "لم يتم اختيار صوت!",
            Time = 5
        })
        return
    end

    local args = {
        [1] = workspace,
        [2] = selectedAudioID,
        [3] = 1
    }
    local GunSound = game:GetService("ReplicatedStorage"):FindFirstChild("1Gu1nSound1s")
    if GunSound then
        pcall(function()
            GunSound:FireServer(unpack(args))
        end)
    end

    task.spawn(function()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://" .. selectedAudioID
        sound.Parent = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") or workspace
        sound:Play()
        task.delay(5, function()
            if sound and sound.Parent then
                sound:Destroy()
            end
        end)
    end)
end

TabMusic:AddButton({
    Name = "تشغيل",
    Callback = function()
        playAudio()
    end
})


TabMusic:AddSection("تكرار أو سبام الصوت")

local audioLoop = false

local function playLoopedAudio()
    if not selectedAudioID then
        TabMusic:AddNotification({
            Name = "تحذير",
            Content = "لم يتم اختيار صوت!",
            Time = 5
        })
        return
    end

    getgenv().ChaosHub_AudioLoop = true

    while getgenv().ChaosHub_AudioLoop do
        local args = {
            [1] = workspace,
            [2] = selectedAudioID,
            [3] = 1
        }
        local GunSound = game:GetService("ReplicatedStorage"):FindFirstChild("1Gu1nSound1s")
        if GunSound then
            pcall(function()
                GunSound:FireServer(unpack(args))
            end)
        end

        task.spawn(function()
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://" .. selectedAudioID
            sound.Parent = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") or workspace
            sound:Play()
            task.delay(0.5, function()
                if sound and sound.Parent then
                    sound:Destroy()
                end
            end)
        end)

        task.wait(0.5)
    end
end

TabMusic:AddToggle({
    Name = "تفعيل اغنية تلقائي",
    Default = false,
    Callback = function(state)
        audioLoop = state
        if audioLoop then
            if not selectedAudioID then
                TabMusic:AddNotification({
                    Name = "خطأ",
                    Content = "اختر صوتًا أولًا!",
                    Time = 5
                })
                LoopSection:Set(false)
                return
            end
            task.spawn(playLoopedAudio)
        else
            getgenv().ChaosHub_AudioLoop = false
        end
    end
})

TabMusic:AddSection("صوت قوي للجميع")

local audioID = 6314880174

local function Audio_All_ClientSide(ID)
    if type(ID) ~= "number" then return end
    task.spawn(function()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://" .. ID
        sound.Volume = 1
        sound.Parent = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") or workspace
        sound:Play()
        task.delay(1, function()
            if sound and sound.Parent then
                sound:Destroy()
            end
        end)
    end)
end

local function Audio_All_ServerSide(ID)
    if type(ID) ~= "number" then return end
    local event = game:GetService("ReplicatedStorage"):FindFirstChild("1Gu1nSound1s")
    if event then
        pcall(function()
            event:FireServer(workspace, ID, 1)
        end)
    end
end

TabMusic:AddToggle({
    Name = "صوت يشگ الاذان",
    Default = false,
    Callback = function(state)
        getgenv().ChaosHub_AudioSpam = state
        while getgenv().ChaosHub_AudioSpam do
            Audio_All_ServerSide(audioID)
            Audio_All_ClientSide(audioID)
            task.wait(0.03)
        end
    end
})

local Tablag = Window:MakeTab({
    Name = "(قنبلة)",
    Icon = "Bomb"
})

Tablag:AddToggle({
    Name = "تفجير تلقائي للقنابل",
    Default = false,
    Callback = function(state)
        if state then
            -- تشغيل الحلقة
            getgenv().AutoBombLoop = true
            task.spawn(function()
                while getgenv().AutoBombLoop do
                    local player = game.Players.LocalPlayer
                    local backpack = player.Backpack
                    local character = player.Character or player.CharacterAdded:Wait()

                    -- نجيب أداة البومب
                    local bombTool = backpack:FindFirstChild("Bomb") or character:FindFirstChild("Bomb")

                    if bombTool then
                        -- نجهز الأداة (Equip)
                        bombTool.Parent = character

                        -- نرمي 10 قنابل متتالية
                        for i = 1, 50 do
                            if not getgenv().AutoBombLoop then break end
                            task.wait(0.01) -- وقت بسيط بين كل قنبلة
                            bombTool:Activate()
                        end
                    end

                    task.wait(1) -- توقف 1 ثانية قبل التكرار (كما في الكود الأصلي)

                    local args = {
                        "BombNetroZHub"
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Blo1wBomb1sServe1r"):FireServer(unpack(args))
                end
            end)
        else
            -- إيقاف الحلقة
            getgenv().AutoBombLoop = false
        end
    end
})

local MusicTab = Window:MakeTab({
    Name = "اغاني عربية",
    Icon = "music"
})

MusicTab:AddSection("ادخل كود الأغنية")

local function PlayMusic(id)
    local args1 = { [1] = "SkateBoard" }
    local args2 = { [1] = "PickingScooterMusicText", [2] = id }
    local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("RE"):FindFirstChild("1NoMoto1rVehicle1s")
    
    if Remote then
        pcall(function()
            Remote:FireServer(unpack(args1))
            Remote:FireServer(unpack(args2))
        end)
    end
end

local LastSongID = nil

MusicTab:AddTextBox({
    Name = "IDادخال",
    Description = "IDفقط",
    PlaceholderText = "07207502:مثل IDاكتب هنا",
    Callback = function(value)
        
        local id = tostring(value):match("%d+")
        if id then
            LastSongID = id
            PlayMusic(id)
        else
            warn("الرجاء إدخال ID صحيح (أرقام فقط)")
        end
    end
})

MusicTab:AddButton({
    "تشغيل",
    function()
        if LastSongID then
            PlayMusic(LastSongID)
        else
            warn("لا يوجد ID محفوظ. أدخل ID أولاً في الـ TextBox.")
        end
    end
})

